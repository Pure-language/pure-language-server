{
  "name": "pure-language-server",
  "displayName": "Pure",
  "description": "Official Pure language extension.",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.70.0"
  },
  "license": "MIT",
  "icon": "assets/logo.png",
  "publisher": "thomasvergne",
  "author": "thomasvergne",
  "repository": "https://github.com/quark-lang/quark-vscode",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pure",
      "aliases": ["Pure", "pure"],
      "extensions": [".pure"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./assets/dark.png",
        "light": "./assets/light.png"
      }
    }],
    "grammars": [{
      "language": "pure",
      "scopeName": "source.pure",
      "path": "./syntaxes/pure.tmLanguage.json"
    }]
  }
}
