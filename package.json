{
  "name": "quark-ls",
  "displayName": "Quark",
  "description": "Official Quark language extension.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "license": "MIT",
  "icon": "assets/logo.png",
  "publisher": "thomasvergne",
  "author": "thomasvergne",
  "repository": "https://github.com/quark-lang/quark-vscode",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "qrk",
      "aliases": ["Quark", "qrk"],
      "extensions": [".qrk"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./assets/dark.png",
        "light": "./assets/light.png"
      }
    }],
    "grammars": [{
      "language": "qrk",
      "scopeName": "source.qrk",
      "path": "./syntaxes/qrk.tmLanguage.json"
    }]
  }
}
